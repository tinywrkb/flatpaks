{
    "name": "sdk-libcups",
    "config-opts": [
        "--libdir=/usr/lib/x86_64-linux-gnu",
        "--disable-gssapi",
        "--disable-libusb"
    ],
    "make-args": [
        "libs"
    ],
    "no-make-install": true,
    "post-install": [
        "make SERVERBIN=/usr/lib/x86_64-linux-gnu/cups install-{headers,libs}"
    ],
    "sources": [
        "sources.json"
    ]
}
